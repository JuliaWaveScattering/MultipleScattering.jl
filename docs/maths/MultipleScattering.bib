	keywords = {Highly concentrated dispersion, Particle characterisation, Reflection measurement setup, Ultrasonic backscattering},
	file = {ScienceDirect Snapshot:/home/art/uom/references/Zotero/storage/HV85RIE6/S0041624X12002090.html:text/html}
}

